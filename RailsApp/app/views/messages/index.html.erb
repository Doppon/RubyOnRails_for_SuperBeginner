<h1>Messages#index</h1>
<p><%= @msg %></p>
<table>
  <tr>
    <th>Id</th>
    <th>title</th>
    <th>person</th>
    <th colspan = "2"></th>
  </tr>
  <% @data.each do |obj| %>
  <tr>
    <td><%= obj.id %></td>
    <td><a href = "/messages/show/<%= obj.id %>"><%= obj.title %></a></td>
    <td><%= obj.person %></td>
    <td><a href = "/messages/edit/<%= obj.id %>">編集</a></td>
    <td><a href = "/messages/delete/<%= obj.id %>">削除</a></td>
  </tr>
  <% end %>
</table>
