<h1>People#index</h1>
<p><%= @msg %></p>
<pre>
  <table>
    <tr>
      <th>id</th>
      <th>name</th>
    </tr>
    <% @data.each do |obj| %>
    <tr>
      <td>
        <%= obj.id %>
      </td>
      <td>
        <a href="/people/<%= obj.id %>"><%= obj.name %></a>
      </td>
    </tr>
    <% end %>
  </table>
</pre>
