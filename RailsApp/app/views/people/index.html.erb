<h1>People#index</h1>
<p><%= @msg %></p>
<pre>
  <table>
    <tr>
      <th>id</th>
      <th>name</th>
      <th colspan = 2></th>
    </tr>
    <% @data.each do |obj| %>
    <tr>
      <td>
        <%= obj.id %>
      </td>
      <td>
        <%= link_to  "#{obj.name}", "/people/#{obj.id}" %>
      </td>
      <td>
        <%= link_to  "編集", "/people/edit/#{obj.id}" %>
      </td>
      <td>
        <%= link_to  "削除", "#" %>
      </td>
    </tr>
    <% end %>
  </table>
</pre>
