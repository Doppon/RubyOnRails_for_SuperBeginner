<h1>People#index</h1>
<p><%= @msg %></p>

<pre>
  <table>
    <tr>
      <th>id</th>
      <th>name</th>
      <th>age</th>
      <th>mail</th>
    </tr>
    <%= @data.each do |obj| %>    
    <tr>
      <td><%= obj.id %></td>
      <td><%= obj.age %></td>
      <td><%= obj.name %></td>
      <td><%= obj.mail %></td>
    </tr>
    <% end %>    
  </table>
</pre>
