<h1>People#index</h1>
<p><%= @msg %></p>

<table>
  <tr>
    <th>id</th>
    <th>name</th>
    <th>age</th>
    <th>mail</th>
  </tr>
  <tr>
    <%= @data.each do |d| %>
      <td><%= d.id %></td>
      <td><%= d.age %></td>
      <td><%= d.name %></td>
      <td><%= d.mail %></td>
    <% end %>    
  </tr>
</table>
